#!/bin/bash

# Sample script that calls taskRankGraphs.py to create graphs of ECResult metrics from results with a recognitionTaskMetrics dict.
# Note: clustering requires a container with sklearn installed.

module add openmind/singularity

checkpoint2='experimentOutputs/list_aic=1.0_arity=4_BO=False_ET=5400_HR=0.5_it=2_MF=5_baseline=False_pc=30.0_RT=7200_storeTaskMetrics=True_L=1.0_taskReranker=default_K=2_topkNotMAP=False_rec=True_feat=LearnedFeatureExtractor_graph=True.pickle'
checkpoint3='experimentOutputs/list_aic=1.0_arity=4_BO=False_ET=5400_HR=0.5_it=3_MF=5_baseline=False_pc=30.0_RT=7200_storeTaskMetrics=True_L=1.0_taskReranker=default_K=2_topkNotMAP=False_rec=True_feat=LearnedFeatureExtractor_graph=True.pickle'
checkpoint4='experimentOutputs/list_aic=1.0_arity=4_BO=False_ET=5400_HR=0.5_it=4_MF=5_baseline=False_pc=30.0_RT=7200_storeTaskMetrics=True_L=1.0_taskReranker=default_K=2_topkNotMAP=False_rec=True_feat=LearnedFeatureExtractor_graph=True.pickle'
checkpoint5='experimentOutputs/list_aic=1.0_arity=4_BO=False_ET=5400_HR=0.5_it=5_MF=5_baseline=False_pc=30.0_RT=7200_storeTaskMetrics=True_L=1.0_taskReranker=default_K=2_topkNotMAP=False_rec=True_feat=LearnedFeatureExtractor_graph=True.pickle'

srun --job-name=graphsHeldout$1 --output=data/heldout --ntasks=1 --mem-per-cpu=5000 --cpus-per-task 1 --time=5:00 --qos=tenenbaum \
  singularity exec -B /om2  sklearn-container.img \
  python taskRankGraphs.py --checkpoints $checkpoint2 $checkpoint3 $checkpoint4 $checkpoint5 --metricsToCluster heldoutTaskLogProductions --export data \
  &

Repeat(Next(Double(unit))) {
    Embed {
        Repeat(Next(Double(unit))) {
            Embed {
                Repeat(Double(Double(unit))) {
                    Integrate(t=Half(unit)) ;
                    Turn
                }
            } ;
            Integrate(pen=off)
        }
    } ;
    Turn ;
    Integrate(pen=off);
    Turn(angle=Opposite(unit))
}
